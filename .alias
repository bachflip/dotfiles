# START download stuff from MARV
MARV_USER=guenther
PW='moped@70.03'
NGROKID='36bc-2a00-1398-200-202-6165-ffd4-2e75-7dc9'

dlMarv () {
	TOKEN=$(curl -s -X POST -H "Content-Type: application/json" \
	  -d '{"username": "'$MARV_USER'", "password": "'$PW'"}' \
	  https://$NGROKID.ngrok.io/marv/api/auth | jq -r .access_token)
	wget --header "Authorization: Bearer $TOKEN" --content-disposition --continue https://$NGROKID.ngrok.io/marv/api/dataset/$1/0
}
# END download stuff from MARV

compdf () {
	gs -sDEVICE=pdfwrite -dCompatibilityLevel=1.4 -dPDFSETTINGS=/prepress \
	-dNOPAUSE -dQUIET -dBATCH -sOutputFile=_$1 $1
}

